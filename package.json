{
  "name": "react-template",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@govtechsg/dnsprove": "^2.5.0",
    "@govtechsg/oa-encryption": "^1.3.3",
    "@govtechsg/oa-verify": "^7.11.0",
    "@govtechsg/open-attestation": "^6.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@types/react-router-dom": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "http-server": "^0.12.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.3.0",
    "postcss-cli": "^8.3.1",
    "postcss-import": "^14.0.2",
    "prettier": "^2.3.0",
    "react-scripts": "^4.0.3",
    "tailwindcss": "^2.1.2",
    "typescript": "^4.2.4",
    "webpack": "4.44.2"
  },
  "scripts": {
    "build": "NODE_ENV=production npm-run-all -s build:css build:js",
    "build:css": "postcss src/tailwind.css -o src/index.css",
    "build:js": "react-scripts build",
    "eject": "react-scripts eject",
    "lint": "eslint ./src --ext .ts,.tsx --max-warnings 0",
    "lint:fix": "npm run lint -- --fix",
    "start": "npm-run-all -p start:*",
    "start:css": "postcss src/tailwind.css -o src/index.css --watch",
    "start:js": "sleep 2 && react-scripts start",
    "test": "react-scripts test"
  },
  "prettier": {
    "printWidth": 120
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
